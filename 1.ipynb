    "    d=n%10\n",
    "    rev=rev*10+d\n",
    "    n=n//10\n",
    "print(rev)\n",
    "if rev==a:\n",
    "    print(a,'is pallindrome')\n",
    "else:\n",
    "    print(a,'is not a pallindrome')"
